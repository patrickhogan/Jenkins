<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This is a copy of the DEV build with 2 additional GCC properties to prevent ads showing (for TESTING ONLY)</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty plugin="build-failure-analyzer@1.6.0">
      <doNotScan>false</doNotScan>
    </com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty>
    <com.coravy.hudson.plugins.github.GithubProjectProperty plugin="github@1.8">
      <projectUrl>https://github.com/channel4/4oD-iOS.git/</projectUrl>
    </com.coravy.hudson.plugins.github.GithubProjectProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.0.4">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/channel4/4oD-iOS.git</url>
        <credentialsId>0f16ad67-9c90-4c7e-b9e3-57d0e8223ab8</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cd C4oD
REVISION=`git log --pretty=format:&apos;&apos; | wc -l | tr -d &apos; &apos;`P
VERSION=`/usr/libexec/PlistBuddy -c &quot;Print :CFBundleShortVersionString&quot; C4oD_iPad-Info.plist`
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleVersion $REVISION&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleVersion&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleIdentifier uk.co.channel4.ios.4od.dev.master&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleIdentifier&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleDisplayName $VERSION-$REVISION&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleDisplayName&quot; C4oD_iPad-Info.plist</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd C4oD
pod install</command>
    </hudson.tasks.Shell>
    <au.com.rayh.XCodeBuilder plugin="xcode-plugin@1.4.2">
      <cleanBeforeBuild>true</cleanBeforeBuild>
      <cleanTestReports>false</cleanTestReports>
      <configuration>Adhoc</configuration>
      <target>C4oD_iPad</target>
      <sdk></sdk>
      <symRoot></symRoot>
      <configurationBuildDir>${WORKSPACE}/output</configurationBuildDir>
      <xcodeProjectPath>C4oD</xcodeProjectPath>
      <xcodeProjectFile></xcodeProjectFile>
      <xcodebuildArguments>PROVISIONING_PROFILE=&quot;401522F4-E9CD-416E-B6F5-1CE1DC8988EA&quot; -sdk iphoneos7.1 ARCHS=&quot;armv7&quot; ONLY_ACTIVE_ARCH=NO GCC_PREPROCESSOR_DEFINITIONS=&quot;BUGSHOTKIT=YES USE_PRODUCTION_RESOURCES=YES DEBUG=YES CRASH_ON_DEMAND=YES TEST_VIDEO_MONITOR=YES CLIENT_PROPERTIES_URL=&apos;ios.int.channel4.com/properties/4od-ios-properties-int-1.json&apos;&quot;</xcodebuildArguments>
      <xcodeSchema>C4oD_iPad</xcodeSchema>
      <xcodeWorkspaceFile>C4oD_iPad</xcodeWorkspaceFile>
      <embeddedProfileFile></embeddedProfileFile>
      <cfBundleVersionValue></cfBundleVersionValue>
      <cfBundleShortVersionStringValue></cfBundleShortVersionStringValue>
      <buildIpa>true</buildIpa>
      <generateArchive>true</generateArchive>
      <unlockKeychain>true</unlockKeychain>
      <keychainName>none (specify one below)</keychainName>
      <keychainPath>/Users/Jenkins/Library/Keychains/login.keychain</keychainPath>
      <keychainPwd>hello</keychainPwd>
      <codeSigningIdentity></codeSigningIdentity>
      <allowFailingBuildResults>false</allowFailingBuildResults>
      <ipaName>4oD-${VERSION}-${BUILD_DATE}</ipaName>
      <ipaOutputDirectory>ipa</ipaOutputDirectory>
      <provideApplicationVersion>false</provideApplicationVersion>
    </au.com.rayh.XCodeBuilder>
    <au.com.rayh.XCodeBuilder plugin="xcode-plugin@1.4.2">
      <cleanBeforeBuild>false</cleanBeforeBuild>
      <cleanTestReports>false</cleanTestReports>
      <configuration>Adhoc</configuration>
      <target>C4oD_iPadTests</target>
      <sdk></sdk>
      <symRoot></symRoot>
      <configurationBuildDir>${WORKSPACE}/output</configurationBuildDir>
      <xcodeProjectPath>C4oD</xcodeProjectPath>
      <xcodeProjectFile></xcodeProjectFile>
      <xcodebuildArguments>test -sdk iphonesimulator7.1 ARCHS=&quot;i386&quot; ONLY_ACTIVE_ARCH=NO GCC_PREPROCESSOR_DEFINITIONS=&quot;USE_PRODUCTION_RESOURCES=YES SKIP_PREROLL_ADVERTS=YES SKIP_BREAK_ADVERTS=YES TEST_VIDEO_MONITOR=YES DEBUG=YES&quot;</xcodebuildArguments>
      <xcodeSchema>C4oD_iPadTestingUnit</xcodeSchema>
      <xcodeWorkspaceFile>C4oD_iPad</xcodeWorkspaceFile>
      <embeddedProfileFile></embeddedProfileFile>
      <cfBundleVersionValue></cfBundleVersionValue>
      <cfBundleShortVersionStringValue></cfBundleShortVersionStringValue>
      <buildIpa>true</buildIpa>
      <generateArchive>false</generateArchive>
      <unlockKeychain>true</unlockKeychain>
      <keychainName>none (specify one below)</keychainName>
      <keychainPath>/Users/Jenkins/Library/Keychains/login.keychain</keychainPath>
      <keychainPwd>hello</keychainPwd>
      <codeSigningIdentity></codeSigningIdentity>
      <allowFailingBuildResults>false</allowFailingBuildResults>
      <ipaName>4oD-${VERSION}-${BUILD_DATE}</ipaName>
      <ipaOutputDirectory></ipaOutputDirectory>
      <provideApplicationVersion>false</provideApplicationVersion>
    </au.com.rayh.XCodeBuilder>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>output/ipa/*.ipa</artifacts>
      <latestOnly>false</latestOnly>
      <allowEmptyArchive>false</allowEmptyArchive>
    </hudson.tasks.ArtifactArchiver>
    <hudson.plugins.textfinder.TextFinderPublisher plugin="text-finder@1.10">
      <regexp>^(?!.*(ThirdParty|Pods)).*(warning\:)</regexp>
      <succeedIfFound>false</succeedIfFound>
      <unstableIfFound>false</unstableIfFound>
      <alsoCheckConsoleOutput>true</alsoCheckConsoleOutput>
    </hudson.plugins.textfinder.TextFinderPublisher>
  </publishers>
  <buildWrappers/>
</project>