<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.coravy.hudson.plugins.github.GithubProjectProperty plugin="github@1.8">
      <projectUrl>https://github.com/channel4/4oD-iOS.git/</projectUrl>
    </com.coravy.hudson.plugins.github.GithubProjectProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.0.4">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/channel4/4oD-iOS.git</url>
        <credentialsId>85272c9d-0f7d-4899-9138-0967c31d5c14</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>*/1 * * * *</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cd C4oD
REVISION=`git log --pretty=format:&apos;&apos; | wc -l | tr -d &apos; &apos;`
VERSION=`/usr/libexec/PlistBuddy -c &quot;Print :CFBundleShortVersionString&quot; C4oD_iPad-Info.plist`
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleVersion $REVISION&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleVersion&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleIdentifier uk.co.channel4.ios.4od.dev.master&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleIdentifier&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleDisplayName 4oD-$VERSION-$REVISION-D&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleDisplayName&quot; C4oD_iPad-Info.plist</command>
    </hudson.tasks.Shell>
    <au.com.rayh.XCodeBuilder plugin="xcode-plugin@1.4.2">
      <cleanBeforeBuild>true</cleanBeforeBuild>
      <cleanTestReports>false</cleanTestReports>
      <configuration>Adhoc</configuration>
      <target>C4oD_iPad</target>
      <sdk></sdk>
      <symRoot></symRoot>
      <configurationBuildDir>${WORKSPACE}/output</configurationBuildDir>
      <xcodeProjectPath></xcodeProjectPath>
      <xcodeProjectFile></xcodeProjectFile>
      <xcodebuildArguments>-sdk iphoneos7.1 ONLY_ACTIVE_ARCH=NO ARCHS=armv7 PROVISIONING_PROFILE=&quot;401522F4-E9CD-416E-B6F5-1CE1DC8988EA&quot; GCC_PREPROCESSOR_DEFINITIONS=&quot;USE_PRODUCTION_RESOURCES=YES MOBILE_DATA_STREAMING_PERMITTED=YES DEBUG=YES CRASH_ON_DEMAND=YES TEST_VIDEO_MONITOR=YES CLIENT_PROPERTIES_URL=&apos;https:/ /github.com/boydallan/scratch/raw/master/ClientProperties/4od-ios-properties-stage-1.json&apos;&quot;</xcodebuildArguments>
      <xcodeSchema>C4oD_iPad</xcodeSchema>
      <xcodeWorkspaceFile>C4oD/C4oD_iPad</xcodeWorkspaceFile>
      <embeddedProfileFile>/Users/jenkins/profiles</embeddedProfileFile>
      <cfBundleVersionValue></cfBundleVersionValue>
      <cfBundleShortVersionStringValue></cfBundleShortVersionStringValue>
      <buildIpa>true</buildIpa>
      <generateArchive>true</generateArchive>
      <unlockKeychain>false</unlockKeychain>
      <keychainName>none (specify one below)</keychainName>
      <keychainPath>address not configured yet &lt;nobody@nowhere&gt;</keychainPath>
      <keychainPwd>hello</keychainPwd>
      <codeSigningIdentity></codeSigningIdentity>
      <allowFailingBuildResults>false</allowFailingBuildResults>
      <ipaName></ipaName>
      <ipaOutputDirectory>ipa</ipaOutputDirectory>
      <provideApplicationVersion>false</provideApplicationVersion>
    </au.com.rayh.XCodeBuilder>
    <au.com.rayh.XCodeBuilder plugin="xcode-plugin@1.4.2">
      <cleanBeforeBuild>false</cleanBeforeBuild>
      <cleanTestReports>true</cleanTestReports>
      <configuration>Adhoc</configuration>
      <target>C4oD_iPadTests</target>
      <sdk></sdk>
      <symRoot></symRoot>
      <configurationBuildDir>${WORKSPACE}/tests</configurationBuildDir>
      <xcodeProjectPath></xcodeProjectPath>
      <xcodeProjectFile></xcodeProjectFile>
      <xcodebuildArguments>test -sdk iphonesimulator7.1 ARCHS=&quot;i386&quot; ONLY_ACTIVE_ARCH=NO GCC_PREPROCESSOR_DEFINITIONS=&quot;USE_PRODUCTION_RESOURCES=YES TEST_VIDEO_MONITOR=YES DEBUG=YES CLIENT_PROPERTIES_URL=&apos;ios.int.channel4.com/properties/4od-ios-properties-int-1.json&apos;&quot;</xcodebuildArguments>
      <xcodeSchema>C4oD_iPadTestingUnit</xcodeSchema>
      <xcodeWorkspaceFile>C4oD/C4oD_iPad</xcodeWorkspaceFile>
      <embeddedProfileFile></embeddedProfileFile>
      <cfBundleVersionValue></cfBundleVersionValue>
      <cfBundleShortVersionStringValue></cfBundleShortVersionStringValue>
      <buildIpa>false</buildIpa>
      <generateArchive>false</generateArchive>
      <unlockKeychain>false</unlockKeychain>
      <keychainName>none (specify one below)</keychainName>
      <keychainPath></keychainPath>
      <keychainPwd></keychainPwd>
      <codeSigningIdentity></codeSigningIdentity>
      <allowFailingBuildResults>false</allowFailingBuildResults>
      <ipaName></ipaName>
      <ipaOutputDirectory></ipaOutputDirectory>
      <provideApplicationVersion>false</provideApplicationVersion>
    </au.com.rayh.XCodeBuilder>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>