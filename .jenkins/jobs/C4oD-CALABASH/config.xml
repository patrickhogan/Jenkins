<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.0.4">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/patrickhogan/4oD-iOS.git</url>
        <credentialsId>a632d6a6-a829-4cf6-b857-a71430989b14</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cd C4oD
REVISION=`git log --pretty=format:&apos;&apos; | wc -l | tr -d &apos; &apos;`
VERSION=`/usr/libexec/PlistBuddy -c &quot;Print :CFBundleShortVersionString&quot; C4oD_iPad-Info.plist`
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleVersion $REVISION&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleVersion&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleIdentifier uk.co.channel4.ios.4od.dev.master&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleIdentifier&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Set :CFBundleDisplayName 4oD-$VERSION-$REVISION-CAL&quot; C4oD_iPad-Info.plist
/usr/libexec/PlistBuddy -c &quot;Print :CFBundleDisplayName&quot; C4oD_iPad-Info.plist</command>
    </hudson.tasks.Shell>
    <au.com.rayh.XCodeBuilder plugin="xcode-plugin@1.4.2">
      <cleanBeforeBuild>true</cleanBeforeBuild>
      <cleanTestReports>false</cleanTestReports>
      <configuration>Adhoc</configuration>
      <target>C4oD_iPad</target>
      <sdk></sdk>
      <symRoot></symRoot>
      <configurationBuildDir>${WORKSPACE}/build-calabash</configurationBuildDir>
      <xcodeProjectPath></xcodeProjectPath>
      <xcodeProjectFile></xcodeProjectFile>
      <xcodebuildArguments>-sdk iphoneos7.1 ONLY_ACTIVE_ARCH=NO ARCHS=armv7 PROVISIONING_PROFILE=&quot;401522F4-E9CD-416E-B6F5-1CE1DC8988EA&quot; GCC_PREPROCESSOR_DEFINITIONS=&quot;USE_PRODUCTION_RESOURCES=YES TEST_VIDEO_MONITOR=YES DEBUG=YES CRASH_ON_DEMAND=YES&quot;</xcodebuildArguments>
      <xcodeSchema>C4oD_iPad-cal</xcodeSchema>
      <xcodeWorkspaceFile>C4oD/C4oD_iPad</xcodeWorkspaceFile>
      <embeddedProfileFile>/Users/jenkins/profiles</embeddedProfileFile>
      <cfBundleVersionValue></cfBundleVersionValue>
      <cfBundleShortVersionStringValue></cfBundleShortVersionStringValue>
      <buildIpa>true</buildIpa>
      <generateArchive>true</generateArchive>
      <unlockKeychain>true</unlockKeychain>
      <keychainName>none (specify one below)</keychainName>
      <keychainPath>${HOME}/Library/Keychains/login.keychain</keychainPath>
      <keychainPwd>hello</keychainPwd>
      <codeSigningIdentity></codeSigningIdentity>
      <allowFailingBuildResults>false</allowFailingBuildResults>
      <ipaName></ipaName>
      <ipaOutputDirectory>ipa</ipaOutputDirectory>
      <provideApplicationVersion>false</provideApplicationVersion>
    </au.com.rayh.XCodeBuilder>
    <hudson.tasks.Shell>
      <command>IPA=&quot;`ls -1t $PWD/*.ipa|head -1`&quot;

echo $PWD

bash -x ./automated_tests/scripts/run_calabash_tests.sh  ./automated_tests/scripts/devices/iphone5_01.config ./automated_tests/ $IPA ./automated_tests/lib/libimobiledevice-macosx/</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>